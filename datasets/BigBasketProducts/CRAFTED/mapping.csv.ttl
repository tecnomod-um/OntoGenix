@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix : <https://vocab.um.es#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMapInvoice> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/Invoice/{FAIR_URI}";
        rr:class :Invoice;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasQuantity;
        rr:objectMap [ rml:reference "Quantity" ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasDiscountPrice;
        rr:objectMap [ rml:reference "DiscountPrice"; rr:datatype xsd:float ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasPrice;
        rr:objectMap [ rml:reference "Price"; rr:datatype xsd:float ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasProduct;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapProduct> ];
    ].

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/Product/{FAIR_URI}";
        rr:class :Product;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasAbsoluteUrl;
        rr:objectMap [ rml:reference "Absolute_Url"; rr:datatype xsd:anyURI ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasFairUri;
        rr:objectMap [ rml:reference "FAIR_URI"; rr:datatype xsd:anyURI ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasImageUrl;
        rr:objectMap [ rml:reference "Image_Url"; rr:datatype xsd:anyURI ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasProductName;
        rr:objectMap [ rml:reference "ProductName"; rr:datatype xsd:string ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasBrand;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapBrand> ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasCategory;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapCategory> ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasSubCategory;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapSubCategory> ];
    ].

<#TriplesMapBrand> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/Brand/{Brand}";
        rr:class :Brand;
    ].

<#TriplesMapCategory> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/Category/{Category}";
        rr:class :Category;
    ].

<#TriplesMapSubCategory> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/SubCategory/{SubCategory}";
        rr:class :SubCategory;
    ].