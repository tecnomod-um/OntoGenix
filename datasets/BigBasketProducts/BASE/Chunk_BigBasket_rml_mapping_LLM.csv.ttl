@prefix um: <https://vocab.um.es#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Product/{FAIR_URI}";
        rr:class um:Product
    ];
    rr:predicateObjectMap [
        rr:predicate um:absoluteUrl;
        rr:objectMap [ rr:column "Absolute_Url" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:fairUri;
        rr:objectMap [ rr:column "FAIR_URI" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:imageUrl;
        rr:objectMap [ rr:column "Image_Url" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:productName;
        rr:objectMap [ rr:column "ProductName" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:quantity;
        rr:objectMap [ rr:column "Quantity" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasBrand;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapBrand>;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasCategory;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapCategory>;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasSubCategory;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapSubCategory>;
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasPriceDetails;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapPriceDetails>;
        ]
    ].

<#TriplesMapBrand> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Brand/{Brand}{FAIR_URI}";
        rr:class um:Brand
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rr:column "Brand" ]
    ].

<#TriplesMapCategory> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Category/{Category}{FAIR_URI}";
        rr:class um:Category
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rr:column "Category" ]
    ].

<#TriplesMapSubCategory> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#SubCategory/{SubCategory}{FAIR_URI}";
        rr:class um:SubCategory
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rr:column "SubCategory" ]
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:subClassOf;
        rr:objectMap [
            rr:constant um:Category
        ]
    ].

<#TriplesMapPriceDetails> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_BigBasket.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#PriceDetails/{FAIR_URI}";
        rr:class um:PriceDetails
    ];
    rr:predicateObjectMap [
        rr:predicate um:discountPrice;
        rr:objectMap [
            rr:column "DiscountPrice";
            rr:datatype xsd:decimal
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:price;
        rr:objectMap [
            rr:column "Price";
            rr:datatype xsd:decimal
        ]
    ].
