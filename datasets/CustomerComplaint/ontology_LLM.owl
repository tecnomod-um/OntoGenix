<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vocab="https://vocab.um.es#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#">

    <owl:Ontology rdf:about="https://vocab.um.es#"/>

    <owl:Class rdf:about="https://vocab.um.es#CustomerComplaint">
        <!-- Object Property Restrictions -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasState"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#State"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasProduct"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#Product"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasProblem"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasCompany"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#Company"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Data Type Property Restrictions -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#dateReceived"/>
                <owl:hasValue rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#narrative"/>
                <owl:hasValue rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>

    </owl:Class>

    <owl:Class rdf:about="https://vocab.um.es#State">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="en">State</rdfs:label>
        <rdfs:comment xml:lang="en">A geographical and administrative division representing states within a country where the complaint originated.</rdfs:comment>

        <!-- Restriction on the State being associated with a CustomerComplaint through hasState object property -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasState"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#CustomerComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="https://vocab.um.es#Product">
        <!-- Hierarchical relationship between Product and SubProduct -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasSubProduct"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#SubProduct"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Data type property indicating the product's name -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#productName"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Object property indicating which complaints are related to this product -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasComplaint"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#CustomerComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="https://vocab.um.es#SubProduct">
        <rdfs:subClassOf rdf:resource="https://vocab.um.es#Product"/>
    </owl:Class>

    <!-- Object Properties and Constraints related to SubProduct -->

    <!-- Relation between Product and SubProduct -->
    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasSubProduct">
        <rdfs:domain rdf:resource="https://vocab.um.es#Product"/>
        <rdfs:range rdf:resource="https://vocab.um.es#SubProduct"/>
    </owl:ObjectProperty>

    <!-- All SubProducts are necessarily associated with a main Product -->
    <owl:Restriction>
        <owl:onProperty rdf:resource="https://vocab.um.es#hasSubProduct"/>
        <owl:someValuesFrom rdf:resource="https://vocab.um.es#SubProduct"/>
        <owl:onClass rdf:resource="https://vocab.um.es#Product"/>
    </owl:Restriction>

    <!-- Data Properties and Constraints related to SubProduct -->

    <!-- Since the insights mention the need for a data type property for each class,
        let's introduce a "description" data property for the SubProduct to describe its characteristics -->

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#subProductDescription">
        <rdfs:domain rdf:resource="https://vocab.um.es#SubProduct"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Ensure that each SubProduct has a description -->
    <owl:Restriction>
        <owl:onProperty rdf:resource="https://vocab.um.es#subProductDescription"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:Restriction>

    <owl:Class rdf:about="https://vocab.um.es#ProblemOfComplaint">
        <!-- Specifying the relationship between ProblemOfComplaint and ProblemSubCategory as per insights -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasSubProblem"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Specifying that every ProblemOfComplaint is potentially part of a CustomerComplaint -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasProblem"/>
                <owl:onClass rdf:resource="https://vocab.um.es#CustomerComplaint"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:Class rdf:about="https://vocab.um.es#ProblemSubCategory">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasSubProblem"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasCompany"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#Company"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <owl:ObjectProperty rdf:about="https://vocab.um.es#relatedToComplaint">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:range rdf:resource="https://vocab.um.es#CustomerComplaint"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="https://vocab.um.es#underProblem">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:range rdf:resource="https://vocab.um.es#ProblemOfComplaint"/>
    </owl:ObjectProperty>

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#description">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#count">
        <rdfs:domain rdf:resource="https://vocab.um.es#ProblemSubCategory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:Class rdf:about="https://vocab.um.es#Company">
        <!-- This class represents companies that can be associated with a CustomerComplaint. -->

        <!-- Relating the company with the complaints filed against it. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasCompany"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#CustomerComplaint"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Relating the company with its respective response to complaints. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasResponse"/>
                <owl:onClass rdf:resource="https://vocab.um.es#CompanyResponse"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- To capture other relevant details about the company, you can add DatatypeProperties like 'name', 'address', 'contactInfo', etc.
             For example, capturing company's name: -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#companyName"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>

    </owl:Class>

    <!-- This is the datatype property to store the company's name. -->
    <owl:DatatypeProperty rdf:about="https://vocab.um.es#companyName">
        <rdfs:domain rdf:resource="https://vocab.um.es#Company"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Entity: CompanyResponse -->
    <owl:Class rdf:about="https://vocab.um.es#CompanyResponse"/>

    <!-- ObjectProperty: hasResolution -->
    <owl:ObjectProperty rdf:about="https://vocab.um.es#hasResolution">
        <rdfs:domain rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <rdfs:range rdf:resource="https://vocab.um.es#Resolution"/>
    </owl:ObjectProperty>

    <!-- DataProperties for CompanyResponse -->
    <owl:DatatypeProperty rdf:about="https://vocab.um.es#response">
        <rdfs:domain rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="https://vocab.um.es#timelyResponse">
        <rdfs:domain rdf:resource="https://vocab.um.es#CompanyResponse"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>

    <!-- Constraints and Relationships for CompanyResponse -->

    <!-- Each CompanyResponse has exactly one Resolution -->
    <rdf:Description rdf:about="https://vocab.um.es#CompanyResponse">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasResolution"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>

    <!-- Each CompanyResponse has exactly one string-based response -->
    <rdf:Description rdf:about="https://vocab.um.es#CompanyResponse">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#response"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>

    <!-- Each CompanyResponse indicates whether the response was timely (as a boolean) -->
    <rdf:Description rdf:about="https://vocab.um.es#CompanyResponse">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#timelyResponse"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </rdf:Description>

    <owl:Class rdf:about="https://vocab.um.es#Resolution">
        <!-- Restriction to specify that every Resolution is associated with only one CompanyResponse -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasResolution"/>
                <owl:onClass rdf:resource="https://vocab.um.es#CompanyResponse"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Restriction to specify that a Resolution has a description, which is a string -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#description"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Data property for the description of a Resolution -->
    <owl:DatatypeProperty rdf:about="https://vocab.um.es#description">
        <rdfs:domain rdf:resource="https://vocab.um.es#Resolution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>



