@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix um: <https://vocab.um.es#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMap_Product> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$"
  ];
  rr:subjectMap [
    rr:template "https://vocab.um.es#Product{Uniq Id}";
    rr:class um:Product
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasBrand;
    rr:objectMap [
      rml:reference "Brand"
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasCategory;
    rr:objectMap [
      rml:reference "Category"
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasPrice;
    rr:objectMap [
      rml:reference "List Price";
      rr:datatype xsd:float
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasPrice;
    rr:objectMap [
      rml:reference "Sale Price";
      rr:datatype xsd:float
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasSize;
    rr:objectMap [
      rml:reference "Package Size"
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasAvailability;
    rr:objectMap [
      rml:reference "Available";
      rr:datatype xsd:boolean
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasDescription;
    rr:objectMap [
      rml:reference "Description"
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate um:hasGtin;
    rr:objectMap [
      rml:reference "Gtin";
      rr:datatype xsd:integer
    ]
  ].

<#TriplesMap_ProductName> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "data.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$"
  ];
  rr:subjectMap [
    rr:template "https://vocab.um.es#Product{Uniq Id}";
    rr:class um:Product
  ];
  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "Product Name"
    ]
  ].
