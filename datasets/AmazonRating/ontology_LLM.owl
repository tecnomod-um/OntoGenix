<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xs="http://www.w3.org/2001/XMLSchema#"
    xmlns:vocab="https://vocab.um.es#">
    xmlns:dc="http://purl.org/dc/elements/1.1/">

    <owl:Ontology rdf:about="https://vocab.um.es#">


        <!-- Definition of the SalesOrder Class -->
        <owl:Class rdf:about="https://vocab.um.es#SalesOrder">
            <!-- SalesOrder is related with SalesArticle through hasProduct property -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasProduct"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#SalesArticle"/>
                </owl:Restriction>
            </rdfs:subClassOf>
            <!-- SalesOrder is related with Customer through hasCustomer property -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasCustomer"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#Customer"/>
                </owl:Restriction>
            </rdfs:subClassOf>
            <!-- Rating property constraints for SalesOrder -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#Rating"/>
                    <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
                </owl:Restriction>
            </rdfs:subClassOf>
            <!-- Timestamp property constraints for SalesOrder -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#Timestamp"/>
                    <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
                </owl:Restriction>
            </rdfs:subClassOf>
        </owl:Class>


        <owl:Class rdf:about="https://vocab.um.es#Customer">
            <!-- Relationship: SalesOrder hasCustomer Customer -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasCustomer"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#SalesOrder"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- DataType Property: UserID with String range -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#UserID"/>
                    <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- Inverse Relationship: SalesOrder hasCustomer Customer -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasCustomer"/>
                    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
                </owl:Restriction>
            </rdfs:subClassOf>
        </owl:Class>

        <owl:Class rdf:about="https://vocab.um.es#SalesArticle">
            <!-- Object Property Restriction: SalesArticle is the range of hasProduct -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasProduct"/>
                    <owl:onClass rdf:resource="https://vocab.um.es#SalesOrder"/>
                    <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minQualifiedCardinality>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- Data Property Restriction: SalesArticle has ProductID of type string -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#ProductID"/>
                    <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                </owl:Restriction>
            </rdfs:subClassOf>
        </owl:Class>

        <owl:ObjectProperty rdf:about="https://vocab.um.es#hasProduct">
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="https://vocab.um.es#SalesArticle"/>

            <!-- task_1: Append relevant metadata and annotations -->
            <rdfs:comment xml:lang="en">This property connects a sales order to the products associated with it.</rdfs:comment>
            <dc:creator>ChatGPT Ontology Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- task_2: Define a description field, propose an alternative name, and alternative labels -->
            <rdfs:label xml:lang="en">hasProduct</rdfs:label>
            <rdfs:label xml:lang="en">containsProduct</rdfs:label>
            <rdfs:label xml:lang="en">includesProduct</rdfs:label>
            <rdfs:label xml:lang="en">productOfOrder</rdfs:label>
            <rdfs:label xml:lang="en">orderProductRelation</rdfs:label>
            <rdfs:label xml:lang="en">orderContains</rdfs:label>
            <rdfs:isDefinedBy xml:lang="en">Relates a sales order to its associated product.</rdfs:isDefinedBy>
            <rdfs:seeAlso rdf:resource="https://vocab.um.es#ProductID"/>
        </owl:ObjectProperty>

        <owl:ObjectProperty rdf:about="https://vocab.um.es#hasCustomer">
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="https://vocab.um.es#Customer"/>

            <!-- task_1: Append relevant metadata and annotations -->
            <rdfs:comment xml:lang="en">This property connects a sales order to the customer who placed it.</rdfs:comment>
            <dc:creator>ChatGPT Ontology Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- task_2: Define a description field, propose an alternative name, and alternative labels -->
            <rdfs:label xml:lang="en">hasCustomer</rdfs:label>
            <rdfs:label xml:lang="en">isOrderedBy</rdfs:label>
            <rdfs:label xml:lang="en">belongsToCustomer</rdfs:label>
            <rdfs:label xml:lang="en">customerOfOrder</rdfs:label>
            <rdfs:label xml:lang="en">orderCustomerRelation</rdfs:label>
            <rdfs:label xml:lang="en">orderPlacedBy</rdfs:label>
            <rdfs:isDefinedBy xml:lang="en">Relates a sales order to its associated customer.</rdfs:isDefinedBy>
            <rdfs:seeAlso rdf:resource="https://vocab.um.es#UserID"/>

        </owl:ObjectProperty>

        <owl:DatatypeProperty rdf:about="https://vocab.um.es#Rating">
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>

            <!-- task_1: Append relevant metadata and annotations -->
            <rdfs:comment xml:lang="en">Represents the rating given to a sales order by the customer or a review system.</rdfs:comment>
            <dc:creator>ChatGPT Ontology Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- task_2: Define a description field, propose an alternative name, and alternative labels -->
            <rdfs:label xml:lang="en">Rating</rdfs:label>
            <rdfs:label xml:lang="en">OrderRating</rdfs:label>
            <rdfs:label xml:lang="en">Score</rdfs:label>
            <rdfs:label xml:lang="en">FeedbackScore</rdfs:label>
            <rdfs:label xml:lang="en">ReviewRating</rdfs:label>
            <rdfs:label xml:lang="en">CustomerScore</rdfs:label>
            <rdfs:isDefinedBy xml:lang="en">Numerical representation of the quality or satisfaction of a sales order.</rdfs:isDefinedBy>

            <!-- task_3: Consider to define equivalent properties -->
            <!-- No equivalent properties defined in the provided ontology -->

        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="https://vocab.um.es#Timestamp">
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>

            <!-- task_1: Append relevant metadata and annotations -->
            <rdfs:comment xml:lang="en">Represents the time at which the sales order was placed or processed.</rdfs:comment>
            <dc:creator>ChatGPT Ontology Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- task_2: Define a description field, propose an alternative name, and alternative labels -->
            <rdfs:label xml:lang="en">Timestamp</rdfs:label>
            <rdfs:label xml:lang="en">OrderTime</rdfs:label>
            <rdfs:label xml:lang="en">OrderTimestamp</rdfs:label>
            <rdfs:label xml:lang="en">OrderDateStamp</rdfs:label>
            <rdfs:label xml:lang="en">SalesOrderTime</rdfs:label>
            <rdfs:label xml:lang="en">PurchaseTime</rdfs:label>
            <rdfs:isDefinedBy xml:lang="en">Exact time representation of when a sales order was processed.</rdfs:isDefinedBy>

            <!-- task_3: Consider to define equivalent properties -->
            <!-- No equivalent properties defined in the provided ontology -->

        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="https://vocab.um.es#UserID">
            <rdfs:domain rdf:resource="https://vocab.um.es#Customer"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>

            <!-- task_1: Append relevant metadata and annotations -->
            <rdfs:comment xml:lang="en">Unique identifier associated with a customer in the system.</rdfs:comment>
            <dc:creator>ChatGPT Ontology Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- task_2: Define a description field, propose an alternative name, and alternative labels -->
            <rdfs:label xml:lang="en">UserID</rdfs:label>
            <rdfs:label xml:lang="en">CustomerID</rdfs:label>
            <rdfs:label xml:lang="en">UserIdentifier</rdfs:label>
            <rdfs:label xml:lang="en">ClientID</rdfs:label>
            <rdfs:label xml:lang="en">CustomerNumber</rdfs:label>
            <rdfs:label xml:lang="en">AccountID</rdfs:label>
            <rdfs:isDefinedBy xml:lang="en">A string-based representation of the unique identity of a customer.</rdfs:isDefinedBy>

            <!-- task_3: Consider to define equivalent properties -->
            <!-- No equivalent properties defined in the provided ontology -->

        </owl:DatatypeProperty>

        <owl:DatatypeProperty rdf:about="https://vocab.um.es#ProductID">
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesArticle"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>

            <!-- task_1: Append relevant metadata and annotations -->
            <rdfs:comment xml:lang="en">Unique identifier associated with a sales article or product in the system.</rdfs:comment>
            <dc:creator>ChatGPT Ontology Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- task_2: Define a description field, propose an alternative name, and alternative labels -->
            <rdfs:label xml:lang="en">ProductID</rdfs:label>
            <rdfs:label xml:lang="en">ArticleID</rdfs:label>
            <rdfs:label xml:lang="en">ItemID</rdfs:label>
            <rdfs:label xml:lang="en">SKU</rdfs:label>
            <rdfs:label xml:lang="en">ProductNumber</rdfs:label>
            <rdfs:label xml:lang="en">ArticleNumber</rdfs:label>
            <rdfs:isDefinedBy xml:lang="en">A string-based representation of the unique identity of a sales article or product.</rdfs:isDefinedBy>

            <!-- task_3: Consider to define equivalent properties -->
            <!-- No equivalent properties defined in the provided ontology -->

        </owl:DatatypeProperty>

    </owl:Ontology>
</rdf:RDF>
