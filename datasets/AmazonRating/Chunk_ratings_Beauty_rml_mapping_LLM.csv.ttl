@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix schema: <http://schema.org/>.
@prefix um: <https://vocab.um.es#>.

<#TriplesMapResource> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Resource/{FAIR_URI}";
        rr:class um:Resource
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasFAIR_URI;
        rr:objectMap [ rml:reference "FAIR_URI" ]
    ].

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Product/{ProductId}";
        rr:class um:Product
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasProductId;
        rr:objectMap [ rml:reference "ProductId" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasReview;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapProductReview>;
        ]
    ].

<#TriplesMapUser> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#User/{UserId}";
        rr:class um:User
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasUserId;
        rr:objectMap [ rml:reference "UserId" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasEvent;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapUserEvent>;
        ]
    ].

<#TriplesMapProductReview> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#ProductReview/{FAIR_URI}";
        rr:class um:ProductReview
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasRating;
        rr:objectMap [
            rml:reference "Rating";
            rr:datatype xsd:integer
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:reviewedBy;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapUser>;
        ]
    ].

<#TriplesMapUserEvent> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#UserEvent/{FAIR_URI}";
        rr:class um:UserEvent
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasTimestamp;
        rr:objectMap [
            rml:reference "Timestamp";
            rr:datatype xsd:dateTimeStamp
        ]
    ].
