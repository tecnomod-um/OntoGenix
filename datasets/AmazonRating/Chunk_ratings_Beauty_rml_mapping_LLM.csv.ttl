@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix um: <https://vocab.um.es#> .
@prefix schema: <http://schema.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<#TriplesMapReview> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Review/{FAIR_URI}";
        rr:class um:Review
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasFAIR_URI;
        rr:objectMap [ rr:column "FAIR_URI" ]
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasReview;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapUser>;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:reviewedProduct;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapProduct>;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasRating;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapRating>;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasTimestamp;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapTimestamp>;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:column "FAIR_URI" ]
    ].

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Product/{ProductId}";
        rr:class um:Product
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasProductId;
        rr:objectMap [ rr:column "ProductId" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:Product;
        rr:objectMap [ rr:column "ProductId" ]
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:column "ProductId" ]
    ].

<#TriplesMapUser> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#User/{UserId}";
        rr:class um:User
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasUserId;
        rr:objectMap [ rr:column "UserId" ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:Person;
        rr:objectMap [ rr:column "UserId" ]
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:column "UserId" ]
    ].

<#TriplesMapRating> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Rating/{FAIR_URI}";
        rr:class um:Rating
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasRatingValue;
        rr:objectMap [
            rr:column "Rating";
            rr:datatype xsd:int
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:Rating;
        rr:objectMap [ rr:column "Rating" ]
    ].

<#TriplesMapTimestamp> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Timestamp/{FAIR_URI}";
        rr:class um:Timestamp
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasTimestampValue;
        rr:objectMap [
            rr:column "Timestamp";
            rr:datatype xsd:dateTimeStamp
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate schema:DateTime;
        rr:objectMap [ rr:column "Timestamp" ]
    ].
