@prefix : <https://vocab.um.es#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMapUser> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#User/{UserId}";
        rr:class :User;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasUserId;
        rr:objectMap [ rml:reference "UserId"; rr:datatype xsd:string ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasRating;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapRating> ];
    ];
    rr:predicateObjectMap [
        rr:predicate :ratedProduct;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapProduct> ];
    ].

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Product/{ProductId}";
        rr:class :Product;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasProductId;
        rr:objectMap [ rml:reference "ProductId"; rr:datatype xsd:string ];
    ].

<#TriplesMapRating> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Rating/{UserId}_{ProductId}";
        rr:class :Rating;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasRatingValue;
        rr:objectMap [ rml:reference "Rating"; rr:datatype xsd:float ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasTimestamp;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapTimestamp> ];
    ].

<#TriplesMapTimestamp> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Timestamp/{UserId}_{ProductId}";
        rr:class :Timestamp;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasTimestampValue;
        rr:objectMap [ rml:reference "Timestamp"; rr:datatype xsd:float ];
    ].
