@prefix : <https://vocab.um.es#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMapUser> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/User/{UserId}{FAIR_URI}";
        rr:class :User;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasUserId;
        rr:objectMap [ rr:column "UserId" ];
    ].

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/Product/{ProductId}{FAIR_URI}";
        rr:class :Product;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasProductId;
        rr:objectMap [ rr:column "ProductId" ];
    ].

<#TriplesMapReview> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es/Review/{FAIR_URI}";
        rr:class :Review;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasFAIR_URI;
        rr:objectMap [ rr:column "FAIR_URI" ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasRating;
        rr:objectMap [ rr:column "Rating"^^xsd:integer ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasTimestamp;
        rr:objectMap [ rr:column "Timestamp"^^xsd:float ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasReview;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapUser>;
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate :reviewOf;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapProduct>;
        ];
    ].
