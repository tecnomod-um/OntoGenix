@prefix : <https://vocab.um.es#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .

<#TriplesMapUser> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#User/{UserId}";
        rr:class :User;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasUserId;
        rr:objectMap [ rr:column "UserId" ];
    ].

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Product/{ProductId}";
        rr:class :Product;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasProductId;
        rr:objectMap [ rr:column "ProductId" ];
    ].

<#TriplesMapRating> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Rating/{UserId}_{ProductId}";
        rr:class :Rating;
    ];
    rr:predicateObjectMap [
        rr:predicate :hasRatingValue;
        rr:objectMap [ rr:column "Rating" ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasTimestamp;
        rr:objectMap [ rr:column "Timestamp" ];
    ];
    rr:predicateObjectMap [
        rr:predicate :hasRated;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapUser>; rr:column "UserId" ];
    ];
    rr:predicateObjectMap [
        rr:predicate :ratedProduct;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapProduct>; rr:column "ProductId" ];
    ].
