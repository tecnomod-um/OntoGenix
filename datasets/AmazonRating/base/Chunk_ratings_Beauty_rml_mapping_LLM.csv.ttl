@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix um: <https://vocab.um.es#> .
@prefix schema: <http://schema.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix XMLSchema: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMapProduct> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Product/{ProductId}";
        rr:class um:Product
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasProductId;
        rr:objectMap [ rml:reference "ProductId" ];
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:constant schema:Product ];
    ].

<#TriplesMapUser> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#User/{UserId}";
        rr:class um:User
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasUserId;
        rr:objectMap [ rml:reference "UserId" ];
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:constant schema:Person ];
    ].

<#TriplesMapReview> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Review/{FAIR_URI}";
        rr:class um:Review
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasFAIR_URI;
        rr:objectMap [ rml:reference "FAIR_URI" ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasReview;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapUser>;
            rml:reference "UserId"
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:reviewedProduct;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapProduct>;
            rml:reference "ProductId"
        ];
    ].

<#TriplesMapRating> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Rating/{FAIR_URI}";
        rr:class um:Rating
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasRatingValue;
        rr:objectMap [
            rr:datatype xsd:int;
            rml:reference "Rating"
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasRating;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapReview>;
            rml:reference "FAIR_URI"
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:constant schema:Rating ];
    ].

<#TriplesMapTimestamp> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_ratings_Beauty.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Timestamp/{FAIR_URI}";
        rr:class um:Timestamp
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasTimestampValue;
        rr:objectMap [
            rr:datatype xsd:dateTimeStamp;
            rml:reference "Timestamp"
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasTimestamp;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapReview>;
            rml:reference "FAIR_URI"
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [ rr:constant XMLSchema:dateTimeStamp ];
    ].
