        <rdfs:label xml:lang="zh">旅行详情</rdfs:label>
        <rdfs:comment xml:lang="en">A property that links a customer to their specific travel details.</rdfs:comment>
        <owl:versionInfo>Version 1.1</owl:versionInfo>
        <dc:creator xml:lang="en">OpenAI Ontology Team</dc:creator>
        <dc:date>2023-10-13</dc:date>

        <!-- Domain and Range -->
        <rdfs:domain rdf:resource="https://vocab.um.es#Customer"/>
        <rdfs:range rdf:resource="https://vocab.um.es#TravelDetails"/>

        <!-- Description Field, Alternative Name and Labels -->
        <dc:description xml:lang="en">This property establishes a relationship between a customer and the specifics of their travel, such as flight distance, type of travel, and flight class.</dc:description>
        <rdfs:altLabel xml:lang="en">holdsTravelDetails</rdfs:altLabel>
        <rdfs:altLabel xml:lang="en">possessesTravelDetails</rdfs:altLabel>
        <rdfs:altLabel xml:lang="en">ownsTravelDetails</rdfs:altLabel>
        <rdfs:altLabel xml:lang="en">containsTravelDetails</rdfs:altLabel>
        <rdfs:altLabel xml:lang="en">maintainsTravelDetails</rdfs:altLabel>

        <!-- Equivalent Properties (For this task, no equivalent properties have been added, as there aren't any known in the ontology) -->
    </owl:ObjectProperty>


