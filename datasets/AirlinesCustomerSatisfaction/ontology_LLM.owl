    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:airline="https://vocab.um.es#">

    <!-- Declaration of Classes -->
    <!-- Entity: Customer -->
    <owl:Class rdf:about="https://vocab.um.es#Customer">
        <!-- Object Properties Restrictions -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasTravelDetails"/>
                <owl:onClass rdf:resource="https://vocab.um.es#TravelDetails"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#TravelDetails"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#gaveFeedbackOn"/>
                <owl:onClass rdf:resource="https://vocab.um.es#EvaluationItem"/>
                <owl:someValuesFrom rdf:resource="https://vocab.um.es#EvaluationItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Data Properties Restrictions -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasAge"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasGender"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasCustomerType"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasSatisfaction"/>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Enhancement of TravelDetails Entity -->
    <owl:Class rdf:about="https://vocab.um.es#TravelDetails">
        <!-- A customer has a single instance of TravelDetails -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasTravelDetails"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Type of Travel is a required attribute for TravelDetails -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasTypeOfTravel"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Flight class is a required attribute for TravelDetails -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasClass"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:minQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Flight Distance should be positive -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasFlightDistance"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>

        <!-- Departure and Arrival delays are represented in minutes and should be non-negative -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasDepartureDelayInMinutes"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://vocab.um.es#hasArrivalDelayInMinutes"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.0</xsd:minInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Declaration of the Class CustomerFeedback -->
    <owl:Class rdf:about="https://vocab.um.es#CustomerFeedback">
        <!-- Linking to the EvaluationItem -->
        <rdfs:subClassOf rdf:resource="https://vocab.um.es#EvaluationItem"/>
