<#TriplesMapAddress> a rr:TriplesMap;
    rml:logicalSource [
        rml:source "Chunk_olist_customers_dataset_no_quotes.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "https://vocab.um.es#Address/{customer_zip_code_prefix}";
        rr:class um:Address
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasCustomerCity;
        rr:objectMap [
            rml:reference "customer_city";
            rr:datatype xsd:string
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasCustomerState;
        rr:objectMap [
            rml:reference "customer_state";
            rr:datatype xsd:string
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate um:hasCustomerZipCodePrefix;
        rr:objectMap [
            rml:reference "customer_zip_code_prefix";
            rr:datatype xsd:integer
        ];
    ].
