<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xs="http://www.w3.org/2001/XMLSchema#"
    xmlns:vocab="https://vocab.um.es#">

    <owl:Ontology rdf:about="https://vocab.um.es#">

        <owl:Class rdf:about="https://vocab.um.es#SalesOrder">
            <!-- Relationship with SalesArticle via hasProduct -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasProduct"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#SalesArticle"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- Relationship with Customer via hasCustomer -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasCustomer"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#Customer"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- Data property constraints for Rating -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#Rating"/>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- Data property constraints for Timestamp -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#Timestamp"/>
                    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
                    <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                </owl:Restriction>
            </rdfs:subClassOf>
        </owl:Class>

        <owl:Class rdf:about="https://vocab.um.es#Customer">
            <!-- Relation to SalesOrder through the object property 'hasCustomer' -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasCustomer"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#SalesOrder"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- UserID Data property restriction -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#UserID"/>
                    <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                </owl:Restriction>
            </rdfs:subClassOf>
        </owl:Class>

        <rdf:Description rdf:about="https://vocab.um.es#SalesArticle">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>

            <!-- Object Property Restriction -->
            <!-- Defining the inverse relationship of hasProduct -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#hasProduct"/>
                    <owl:someValuesFrom rdf:resource="https://vocab.um.es#SalesOrder"/>
                </owl:Restriction>
            </rdfs:subClassOf>

            <!-- Data Property Restrictions -->

            <!-- Defining the ProductID restriction for SalesArticle -->
            <rdfs:subClassOf>
                <owl:Restriction>
                    <owl:onProperty rdf:resource="https://vocab.um.es#ProductID"/>
                    <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                    <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
                </owl:Restriction>
            </rdfs:subClassOf>
        </rdf:Description>

        <rdf:Description rdf:about="https://vocab.um.es#hasProduct">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>

            <!-- Task 1: Metadata and Annotations -->
            <rdfs:comment>This property relates a SalesOrder to a specific SalesArticle which it is associated with.</rdfs:comment>
            <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
            <owl:versionInfo>Version 1.0</owl:versionInfo>
            <dc:creator xml:lang="en">Ontology Design Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- Task 2: Description, Alternative Name and Labels -->
            <rdfs:label>has Product</rdfs:label>
            <skos:altLabel>contains Product</skos:altLabel>
            <skos:altLabel>includes Product</skos:altLabel>
            <skos:altLabel>product of the order</skos:altLabel>
            <skos:altLabel>product in order</skos:altLabel>
            <skos:altLabel>associated product</skos:altLabel>
            <skos:prefLabel>has Product</skos:prefLabel>
            <skos:definition>The object property that associates a sales order with a particular product or sales article.</skos:definition>
            <skos:notation>HPROD</skos:notation>

            <!-- Task 3: Equivalent Properties -->
            <!-- No known equivalent properties, so none are defined -->

            <!-- Original Definitions from Entity -->
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="https://vocab.um.es#SalesArticle"/>
        </rdf:Description>


        <rdf:Description rdf:about="https://vocab.um.es#hasCustomer">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>

            <!-- Task 1: Metadata and Annotations -->
            <rdfs:comment>This property relates a SalesOrder to the Customer who made the order.</rdfs:comment>
            <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
            <owl:versionInfo>Version 1.0</owl:versionInfo>
            <dc:creator xml:lang="en">Ontology Design Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- Task 2: Description, Alternative Name and Labels -->
            <rdfs:label>has Customer</rdfs:label>
            <skos:altLabel>related Customer</skos:altLabel>
            <skos:altLabel>Customer of Order</skos:altLabel>
            <skos:altLabel>order's Customer</skos:altLabel>
            <skos:altLabel>associated Customer</skos:altLabel>
            <skos:altLabel>belongs to Customer</skos:altLabel>
            <skos:prefLabel>has Customer</skos:prefLabel>
            <skos:definition>The object property that connects a sales order to its respective customer.</skos:definition>
            <skos:notation>HCUST</skos:notation>

            <!-- Task 3: Equivalent Properties -->
            <!-- No known equivalent properties, so none are defined -->

            <!-- Original Definitions from Entity -->
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="https://vocab.um.es#Customer"/>
        </rdf:Description>


        <rdf:Description rdf:about="https://vocab.um.es#Rating">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>

            <!-- Task 1: Metadata and Annotations -->
            <rdfs:comment>This property indicates the rating given to a SalesOrder.</rdfs:comment>
            <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
            <owl:versionInfo>Version 1.0</owl:versionInfo>
            <dc:creator xml:lang="en">Ontology Design Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- Task 2: Description, Alternative Name and Labels -->
            <rdfs:label>Rating</rdfs:label>
            <skos:altLabel>Order Rating</skos:altLabel>
            <skos:altLabel>SalesOrder Score</skos:altLabel>
            <skos:altLabel>Feedback Score</skos:altLabel>
            <skos:altLabel>Review Rating</skos:altLabel>
            <skos:altLabel>Customer Rating</skos:altLabel>
            <skos:prefLabel>Rating of SalesOrder</skos:prefLabel>
            <skos:definition>The numeric property that represents the rating score given to a sales order.</skos:definition>
            <skos:notation>RTNG</skos:notation>

            <!-- Task 3: Equivalent Properties -->
            <!-- No known equivalent properties, so none are defined -->

            <!-- Original Definitions from Entity -->
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        </rdf:Description>


        <rdf:Description rdf:about="https://vocab.um.es#Timestamp">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>

            <!-- Task 1: Metadata and Annotations -->
            <rdfs:comment>This property indicates the timestamp associated with a SalesOrder.</rdfs:comment>
            <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
            <owl:versionInfo>Version 1.0</owl:versionInfo>
            <dc:creator xml:lang="en">Ontology Design Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- Task 2: Description, Alternative Name and Labels -->
            <rdfs:label>Timestamp</rdfs:label>
            <skos:altLabel>Order Timestamp</skos:altLabel>
            <skos:altLabel>SalesOrder Time</skos:altLabel>
            <skos:altLabel>Order Timepoint</skos:altLabel>
            <skos:altLabel>Creation Time</skos:altLabel>
            <skos:altLabel>Transaction Time</skos:altLabel>
            <skos:prefLabel>Timestamp of SalesOrder</skos:prefLabel>
            <skos:definition>The numeric property that represents the exact point in time a sales order was created.</skos:definition>
            <skos:notation>TSMP</skos:notation>

            <!-- Task 3: Equivalent Properties -->
            <!-- No known equivalent properties, so none are defined -->

            <!-- Original Definitions from Entity -->
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesOrder"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        </rdf:Description>


        <rdf:Description rdf:about="https://vocab.um.es#UserID">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>

            <!-- Task 1: Metadata and Annotations -->
            <rdfs:comment>This property indicates the unique user identification associated with a Customer.</rdfs:comment>
            <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
            <owl:versionInfo>Version 1.0</owl:versionInfo>
            <dc:creator xml:lang="en">Ontology Design Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- Task 2: Description, Alternative Name and Labels -->
            <rdfs:label>UserID</rdfs:label>
            <skos:altLabel>User Identifier</skos:altLabel>
            <skos:altLabel>Customer ID</skos:altLabel>
            <skos:altLabel>User Key</skos:altLabel>
            <skos:altLabel>Member ID</skos:altLabel>
            <skos:altLabel>Account Number</skos:altLabel>
            <skos:prefLabel>Primary User Identification</skos:prefLabel>
            <skos:definition>A string-based property that uniquely identifies a customer within the system.</skos:definition>
            <skos:notation>UID</skos:notation>

            <!-- Task 3: Equivalent Properties -->
            <!-- No known equivalent properties, so none are defined -->

            <!-- Original Definitions from Entity -->
            <rdfs:domain rdf:resource="https://vocab.um.es#Customer"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </rdf:Description>


        <rdf:Description rdf:about="https://vocab.um.es#ProductID">
            <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>

            <!-- Task 1: Metadata and Annotations -->
            <rdfs:comment>This property indicates the unique product identification associated with a SalesArticle.</rdfs:comment>
            <rdfs:isDefinedBy rdf:resource="https://vocab.um.es#"/>
            <owl:versionInfo>Version 1.0</owl:versionInfo>
            <dc:creator xml:lang="en">Ontology Design Team</dc:creator>
            <dc:date>2023-10-11</dc:date>

            <!-- Task 2: Description, Alternative Name and Labels -->
            <rdfs:label>ProductID</rdfs:label>
            <skos:altLabel>Article Identifier</skos:altLabel>
            <skos:altLabel>Product Key</skos:altLabel>
            <skos:altLabel>Item Code</skos:altLabel>
            <skos:altLabel>SKU Number</skos:altLabel>
            <skos:altLabel>Barcode</skos:altLabel>
            <skos:prefLabel>Primary Product Code</skos:prefLabel>
            <skos:definition>A string-based property that uniquely identifies a sales article or product within the system.</skos:definition>
            <skos:notation>PID</skos:notation>

            <!-- Task 3: Equivalent Properties -->
            <!-- No known equivalent properties, so none are defined -->

            <!-- Original Definitions from Entity -->
            <rdfs:domain rdf:resource="https://vocab.um.es#SalesArticle"/>
            <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        </rdf:Description>


    </owl:Ontology>
</rdf:RDF>
